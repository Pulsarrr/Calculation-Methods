function[f4, x0] = HW1_Lagrange_interpolation(f1, x1, x0)
    f4=zeros(length(x0), 1);
    for j=(1:1:length(x0))
        L=0;
        for k=(0:1:length(x1))
            I=1;
            for i=(0:1:length(x1))
                if i~=k
                    I = I*((x0(j)-x1(i))/(x1(k)-x1(i)));
                end
            end
            L = L+f1(k)*I;
        end
        f4(j)=L;
    end
end
